{
    "final_space": true,
    "console_title": true,
    "console_title_style": "folder",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "newline": true,
            "segments": [
                {
                    "type": "root",
                    "style": "diamond",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "invert_powerline": true,
                    "foreground": "#ffffff",
                    "background": "#1f6187",
                    "properties": {
                        "root_icon": "\uF0E7"
                    }
                },
                {
                    "type": "path",
                    "style": "diamond",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "background": "#3984af",
                    "foreground": "#ffffff",
                    "properties": {
                        "prefix": "  ",
                        "style": "folder"
                    }
                },
                // {
                //     "type": "az",
                //     "style": "powerline",
                //     "powerline_symbol": "",
                //     "foreground": "#000000",
                //     "background": "#9ec3f0",
                //     "properties": {
                //       "template": "{{.Name}}",
                //       "prefix": " \uFD03 "
                //     }
                // },
                {
                    "type": "dotnet",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "background": "#9D56F3",
                    "foreground": "#ffffff",
                    "properties": {
                        "display_version": true,
                        "prefix": "  "
                    }
                },
                {
                    "type": "envvar",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "background": "#a767f4",
                    "foreground": "#ffffff",
                    "properties": {
                      "var_name": "ASPNETCORE_ENVIRONMENT"
                    }
                },
                {
                    "type": "executiontime",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "background": "#b178f5",
                    "foreground": "#ffffff",
                    "properties": {
                      "threshold": 500,
                      "style": "austin",
                      "prefix": " 祥 "
                    }
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ffffff",
                    "background": "#87b576",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#E5C07B{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#E8a879{{ end }}",
                        "{{ if gt .Ahead 0 }}#E8a879{{ end }}",
                        "{{ if gt .Behind 0 }}#E06C75{{ end }}"
                    ],
                    "properties": {
                        "display_upstream_icon": true,
                        "display_status": true,
                        "display_stash_count": true,
                        "fetch_status": true,
                        "fetch_stash_count": true
                    }
                },
                {
                    "type": "exit",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ffffff",
                    "background": "#6CA35E",
                    "properties": {
                        "always_enabled": true,
                        "color_background": true,
                        "display_exit_code": false,
                        "error_color": "#E06C75",
                        "prefix": " "
                    }
                }
            ]
        }
    ]
}